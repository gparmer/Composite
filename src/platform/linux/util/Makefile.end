reset: clean cp build

init: first

first: cp build

cp:
	rm *.o
	cp $(TRANS_DIR)/* $(TEST_DIR)

clean:
	rm cos_loader
	rmmod trans
	rmmod cnet
	rmmod cos

# sync to minimize possible file system corruption on crashes
build: 
	sync
	sleep 1
	nice -n -20 insmod cos.ko
#	sleep 1
	insmod cnet.ko
	insmod trans.ko
	./cnet_user
